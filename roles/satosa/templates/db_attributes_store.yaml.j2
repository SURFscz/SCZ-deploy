module: satosa.micro_services.db_attribute_store.DBAttributeStore
name: DBAttributeStore
config:
  db_host: 'localhost'
  db_user: '{{comanage_proxy_db_user}}'
  db_schema: '{{comanage_proxy_db_name}}'
  db_password: '{{comanage_proxy_db_user}}'
  idp_identifiers:
    - eduPersonPrincipalName
    #- uid

  # Whether to clear values for attributes incoming
  # to this microservice. Default is no or false.
  clear_input_attributes: yes
